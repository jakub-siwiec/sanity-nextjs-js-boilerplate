{
  "name": "sanity-nextjs-js-boilerplate",
  "private": true,
  "workspaces": [
    "studio"
  ],
  "scripts": {
    "dev": "concurrently \"next dev\" \"cd studio && npm run start\"",
    "build": "echo 'Building Sanity to public/studio' && cd studio && npm run build && cd .. && next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@sanity/cli": "^2.22.3",
    "next": "12.0.4",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "concurrently": "^6.4.0",
    "eslint": "7.32.0",
    "eslint-config-next": "12.0.4"
  }
}
